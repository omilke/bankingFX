<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<DialogPane fx:id="dialog" contentText="Select the Entry Sequence" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="285.0"
            xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.omilke.bankingfx.main.sequenceeditor.SequenceeditorView">
    <buttonTypes>
        <ButtonType fx:constant="APPLY"/>
        <ButtonType fx:constant="CANCEL"/>
    </buttonTypes>
    <content>
        <GridPane hgap="10.0">
            <columnConstraints>
                <ColumnConstraints fillWidth="false" halignment="RIGHT" maxWidth="135.0" minWidth="72.0"
                                   prefWidth="90.0"/>
                <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" maxWidth="-Infinity" minWidth="-Infinity"
                                   prefWidth="175.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <ChoiceBox fx:id="sequence" prefWidth="175.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"/>
                <TextField fx:id="order" maxWidth="-Infinity" prefWidth="175.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="1"/>
                <Label prefHeight="16.0" prefWidth="67.0" text="Sequence"/>
                <Label text="Order Index" GridPane.rowIndex="1"/>
            </children>
        </GridPane>
    </content>
</DialogPane>
